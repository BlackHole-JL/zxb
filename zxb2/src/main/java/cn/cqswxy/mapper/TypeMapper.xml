<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cqswxy.mapper.TypeMapper">
	
	<select id="selectAll" resultType="cn.cqswxy.entity.Type">
		SELECT * FROM tb_type
	</select>
	
	<select id="selectById" resultType="cn.cqswxy.entity.Type">
		SELECT * FROM tb_type WHERE typeId = #{typeId}
	</select>
	
</mapper>